.basicPanel {
    width: auto;
    height: auto;
    background-color: rgba(61, 85, 101, 0.51);
    border-left-color: rgb(154, 215, 255);
    border-right-color: rgb(154, 215, 255);
    border-top-color: rgb(154, 215, 255);
    border-bottom-color: rgb(154, 215, 255);
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 10px;
    margin-bottom: 10px;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 5px;
    padding-bottom: 5px;
    -unity-font: resource('FlatSkin/Font/Roboto-Light');
    left: 0;
    top: 0;
    flex-shrink: 1;
}

.basicBtn {
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 3px;
    margin-bottom: 3px;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 2px;
    padding-bottom: 2px;
    flex-shrink: 1;
    flex-grow: 0;
    width: auto;
    height: auto;
    -unity-font: resource('FlatSkin/Font/Roboto-Bold');
    -unity-font-style: normal;
    font-size: 14px;
    -unity-text-align: middle-center;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.row {
    flex-direction: row;
    align-items: flex-start;
    justify-content: space-between;
    flex-grow: 0;
    flex-shrink: 0;
    width: 100%;
}

.box-label {
    opacity: 1;
    font-size: 14px;
    -unity-font: resource('FlatSkin/Font/Roboto-Medium');
    -unity-text-align: upper-center;
    -unity-text-outline-width: 0;
    text-shadow: 0 0 0 rgba(46, 46, 46, 0.87);
    letter-spacing: 1px;
    color: rgb(154, 215, 255);
    background-color: rgba(255, 255, 255, 0);
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
    margin-left: 40px;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    border-bottom-width: 2px;
    border-left-color: rgb(154, 215, 255);
    border-right-color: rgb(154, 215, 255);
    border-top-color: rgb(154, 215, 255);
    border-bottom-color: rgb(154, 215, 255);
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    scale: 1 1;
    flex-shrink: 1;
    flex-grow: 0;
    align-items: flex-start;
    justify-content: flex-start;
    width: 50%;
    border-right-width: 1px;
}

.label {
    color: rgb(154, 215, 255);
    border-left-color: rgb(90, 125, 148);
    border-right-color: rgb(90, 125, 148);
    border-top-color: rgb(90, 125, 148);
    border-bottom-color: rgb(90, 125, 148);
    border-bottom-width: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    -unity-font: resource('FlatSkin/Font/Roboto-Light');
    border-right-width: 5px;
    border-left-width: 0;
    padding-left: 7px;
    padding-right: 7px;
    background-color: rgb(66, 92, 109);
    padding-top: 2px;
    padding-bottom: 2px;
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 0;
    margin-bottom: 2px;
    align-items: center;
    flex-shrink: 0;
    flex-direction: row;
    justify-content: space-between;
    flex-grow: 1;
    width: 100%;
}

.cardBody {
    align-items: flex-start;
