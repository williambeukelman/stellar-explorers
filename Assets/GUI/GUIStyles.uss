#OrangeScrollGrid .unity-base-slider__dragger{
    background-color: rgb(228, 155, 100);
}

.unity-base-slider__dragger {
    background-color: rgb(154, 215, 255);
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    width: 5px;
}

.unity-scroller--vertical .unity-base-slider__tracker {
    background-color: rgba(69, 96, 114, 0);
    border-left-width: 0;
    border-right-width: 0;
    width: 5px;
}

#unity-low-button {
    background-image: none;
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    width: 0;
    height: 0;
}

#unity-high-button {
    background-image: none;
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    width: 0;
    height: 0;
}

.unity-base-field:focus > .unity-base-field__label {
    color: rgb(179, 225, 255);
}

.unity-radio-button__text {
    margin-left: 10px;
    padding-bottom: 0;
    padding-top: 5px;
    font-size: 20px;
    -unity-font-style: normal;
}

.unity-radio-button__checkmark {
    background-color: rgba(154, 215, 255, 0.31);
    border-top-left-radius: 10px;
    border-bottom-left-radius: 0.1px;
    border-top-right-radius: 0.1px;
    border-bottom-right-radius: 0.1px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    width: 100%;
    height: 122%;
    align-items: auto;
    justify-content: flex-start;
    flex-direction: column;
    -unity-text-align: upper-left;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    top: -4px;
}

.unity-radio-button__checkmark-background {
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 10px;
    border-width: 1px;
    border-left-color: rgb(179, 225, 255);
    border-right-color: rgb(179, 225, 255);
    border-top-color: rgb(179, 225, 255);
    border-bottom-color: rgb(179, 225, 255);
    width: 100%;
    position: absolute;
    background-color: rgba(179, 225, 255, 0);
    border-top-right-radius: 0;
    border-top-left-radius: 10px;
    padding-left: 0;
    padding-right: 0;
    margin-left: 0;
}

